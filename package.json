    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.2.3",
    "estraverse-fb": "^1.3.1",
    "joi": "^5.1.0",
    "jscs": "^2.11.0",
    "lab": "^10.3.0",
    "lab-babel": "^1.1.1",
    "sinon": "^1.17.3"
  }
