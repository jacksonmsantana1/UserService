  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "stage": 0
  },
  "dependencies": {
    "boom": "^3.1.2",
    "good": "^6.6.0",
    "good-file": "^5.1.2",
    "hapi": "^13.3.0",
    "hapi-auth-jwt": "^4.0.0",
    "hapi-mongodb": "^6.0.0",
    "joi": "^5.1.0",
    "jsonwebtoken": "^5.7.0",
    "mongodb": "^2.1.16",
